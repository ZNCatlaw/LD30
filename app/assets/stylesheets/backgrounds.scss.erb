#background-images > div{
  position: absolute;
  top: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  margin: 0;
  padding: 0;

  z-index: -5;
  overflow: hidden;

  background: rgba(0,0,0,1);
  background-size: cover;
  background-position: center;

  @include transform( translate3d(0, 0, 0) );
  filter: blur(1px);
  -webkit-filter: blur(1px);
  -ms-filter: blur(1px);

  &.bg{
    background-image: url(<%= asset_path('bg.jpg') %>);
  }

  &.bg-0{
    background-image: url(<%= asset_path('bg-0.jpg') %>);
  }

  &.bg-9{
    background-image: url(<%= asset_path('bg-9.jpg') %>);
  }

  &.bg-10{
    background-image: url(<%= asset_path('bg-10.jpg') %>);
  }

  &.bg-11{
    background-image: url(<%= asset_path('bg-11.jpg') %>);
  }

  &.bg-13{
    background-image: url(<%= asset_path('bg-13.jpg') %>);
  }

  &.bg-20{
    background-image: url(<%= asset_path('bg-20.jpg') %>);
  }

  &.bg-21{
    background-image: url(<%= asset_path('bg-21.jpg') %>);
  }
}
